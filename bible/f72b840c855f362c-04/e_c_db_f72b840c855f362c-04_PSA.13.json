{"_default": {"1": {"id": "PSA.13", "bibleId": "f72b840c855f362c-04", "number": "13", "bookId": "PSA", "reference": "Psalms 13", "copyright": "\n          \n            PUBLIC DOMAIN\n          \n        ", "content": [{"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "How long, LORD?", "type": "text", "attrs": {"verseId": "PSA.13.1", "verseOrgIds": ["PSA.13.2"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 13:1"}, "items": [{"text": "Will you forget me forever?", "type": "text", "attrs": {"verseId": "PSA.13.1", "verseOrgIds": ["PSA.13.2"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 13:1"}, "items": [{"text": "How long will you hide your face from me?", "type": "text", "attrs": {"verseId": "PSA.13.1", "verseOrgIds": ["PSA.13.2"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "How long shall I take counsel in my soul,", "type": "text", "attrs": {"verseId": "PSA.13.2", "verseOrgIds": ["PSA.13.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 13:2"}, "items": [{"text": "having sorrow in my heart every day?", "type": "text", "attrs": {"verseId": "PSA.13.2", "verseOrgIds": ["PSA.13.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 13:2"}, "items": [{"text": "How long shall my enemy triumph over me?", "type": "text", "attrs": {"verseId": "PSA.13.2", "verseOrgIds": ["PSA.13.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Behold, and answer me, LORD, my God.", "type": "text", "attrs": {"verseId": "PSA.13.3", "verseOrgIds": ["PSA.13.4"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 13:3"}, "items": [{"text": "Give light to my eyes, lest I sleep in death;", "type": "text", "attrs": {"verseId": "PSA.13.3", "verseOrgIds": ["PSA.13.4"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2"}, "items": [{"text": "lest my enemy say, \u201cI have prevailed against him;\u201d", "type": "text", "attrs": {"verseId": "PSA.13.4", "verseOrgIds": ["PSA.13.5"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 13:4"}, "items": [{"text": "lest my adversaries rejoice when I fall.", "type": "text", "attrs": {"verseId": "PSA.13.4", "verseOrgIds": ["PSA.13.5"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "b"}, "items": []}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "But I trust in your loving kindness.", "type": "text", "attrs": {"verseId": "PSA.13.5", "verseOrgIds": ["PSA.13.6"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 13:5"}, "items": [{"text": "My heart rejoices in your salvation.", "type": "text", "attrs": {"verseId": "PSA.13.5", "verseOrgIds": ["PSA.13.6"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "I will sing to the LORD,", "type": "text", "attrs": {"verseId": "PSA.13.6", "verseOrgIds": ["PSA.13.6"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 13:6"}, "items": [{"text": "because he has been good to me.", "type": "text", "attrs": {"verseId": "PSA.13.6", "verseOrgIds": ["PSA.13.6"]}}]}], "next": {"id": "PSA.14", "number": "14", "bookId": "PSA"}, "previous": {"id": "PSA.12", "number": "12", "bookId": "PSA"}}}}