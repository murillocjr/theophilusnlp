{"_default": {"1": {"id": "PSA.6", "bibleId": "f72b840c855f362c-04", "number": "6", "bookId": "PSA", "reference": "Psalms 6", "copyright": "\n          \n            PUBLIC DOMAIN\n          \n        ", "content": [{"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "LORD, don\u2019t rebuke me in your anger,", "type": "text", "attrs": {"verseId": "PSA.6.1", "verseOrgIds": ["PSA.6.2"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 6:1"}, "items": [{"text": "neither discipline me in your wrath.", "type": "text", "attrs": {"verseId": "PSA.6.1", "verseOrgIds": ["PSA.6.2"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Have mercy on me, LORD, for I am faint.", "type": "text", "attrs": {"verseId": "PSA.6.2", "verseOrgIds": ["PSA.6.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 6:2"}, "items": [{"text": "LORD, heal me, for my bones are troubled.", "type": "text", "attrs": {"verseId": "PSA.6.2", "verseOrgIds": ["PSA.6.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "My soul is also in great anguish.", "type": "text", "attrs": {"verseId": "PSA.6.3", "verseOrgIds": ["PSA.6.4"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 6:3"}, "items": [{"text": "But you, LORD\u2014how long?", "type": "text", "attrs": {"verseId": "PSA.6.3", "verseOrgIds": ["PSA.6.4"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Return, LORD. Deliver my soul,", "type": "text", "attrs": {"verseId": "PSA.6.4", "verseOrgIds": ["PSA.6.5"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 6:4"}, "items": [{"text": "and save me for your loving kindness\u2019 sake.", "type": "text", "attrs": {"verseId": "PSA.6.4", "verseOrgIds": ["PSA.6.5"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "For in death there is no memory of you.", "type": "text", "attrs": {"verseId": "PSA.6.5", "verseOrgIds": ["PSA.6.6"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 6:5"}, "items": [{"text": "In Sheol,", "type": "text", "attrs": {"verseId": "PSA.6.5", "verseOrgIds": ["PSA.6.6"]}}, {"text": " who shall give you thanks?", "type": "text", "attrs": {"verseId": "PSA.6.5", "verseOrgIds": ["PSA.6.6"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "I am weary with my groaning.", "type": "text", "attrs": {"verseId": "PSA.6.6", "verseOrgIds": ["PSA.6.7"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 6:6"}, "items": [{"text": "Every night I flood my bed.", "type": "text", "attrs": {"verseId": "PSA.6.6", "verseOrgIds": ["PSA.6.7"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 6:6"}, "items": [{"text": "I drench my couch with my tears.", "type": "text", "attrs": {"verseId": "PSA.6.6", "verseOrgIds": ["PSA.6.7"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "My eye wastes away because of grief.", "type": "text", "attrs": {"verseId": "PSA.6.7", "verseOrgIds": ["PSA.6.8"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 6:7"}, "items": [{"text": "It grows old because of all my adversaries.", "type": "text", "attrs": {"verseId": "PSA.6.7", "verseOrgIds": ["PSA.6.8"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Depart from me, all you workers of iniquity,", "type": "text", "attrs": {"verseId": "PSA.6.8", "verseOrgIds": ["PSA.6.9"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 6:8"}, "items": [{"text": "for the LORD has heard the voice of my weeping.", "type": "text", "attrs": {"verseId": "PSA.6.8", "verseOrgIds": ["PSA.6.9"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "The LORD has heard my supplication.", "type": "text", "attrs": {"verseId": "PSA.6.9", "verseOrgIds": ["PSA.6.10"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 6:9"}, "items": [{"text": "The LORD accepts my prayer.", "type": "text", "attrs": {"verseId": "PSA.6.9", "verseOrgIds": ["PSA.6.10"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "May all my enemies be ashamed and dismayed.", "type": "text", "attrs": {"verseId": "PSA.6.10", "verseOrgIds": ["PSA.6.11"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 6:10"}, "items": [{"text": "They shall turn back, they shall be disgraced suddenly.", "type": "text", "attrs": {"verseId": "PSA.6.10", "verseOrgIds": ["PSA.6.11"]}}]}], "next": {"id": "PSA.7", "number": "7", "bookId": "PSA"}, "previous": {"id": "PSA.5", "number": "5", "bookId": "PSA"}}}}