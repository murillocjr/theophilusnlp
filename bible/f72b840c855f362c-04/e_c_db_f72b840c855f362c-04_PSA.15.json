{"_default": {"1": {"id": "PSA.15", "bibleId": "f72b840c855f362c-04", "number": "15", "bookId": "PSA", "reference": "Psalms 15", "copyright": "\n          \n            PUBLIC DOMAIN\n          \n        ", "content": [{"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "LORD, who shall dwell in your sanctuary?", "type": "text", "attrs": {"verseId": "PSA.15.1", "verseOrgIds": ["PSA.15.1"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 15:1"}, "items": [{"text": "Who shall live on your holy hill?", "type": "text", "attrs": {"verseId": "PSA.15.1", "verseOrgIds": ["PSA.15.1"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "He who walks blamelessly and does what is right,", "type": "text", "attrs": {"verseId": "PSA.15.2", "verseOrgIds": ["PSA.15.2"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 15:2"}, "items": [{"text": "and speaks truth in his heart;", "type": "text", "attrs": {"verseId": "PSA.15.2", "verseOrgIds": ["PSA.15.2"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "he who doesn\u2019t slander with his tongue,", "type": "text", "attrs": {"verseId": "PSA.15.3", "verseOrgIds": ["PSA.15.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 15:3"}, "items": [{"text": "nor does evil to his friend,", "type": "text", "attrs": {"verseId": "PSA.15.3", "verseOrgIds": ["PSA.15.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 15:3"}, "items": [{"text": "nor casts slurs against his fellow man;", "type": "text", "attrs": {"verseId": "PSA.15.3", "verseOrgIds": ["PSA.15.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "in whose eyes a vile man is despised,", "type": "text", "attrs": {"verseId": "PSA.15.4", "verseOrgIds": ["PSA.15.4"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 15:4"}, "items": [{"text": "but who honors those who fear the LORD;", "type": "text", "attrs": {"verseId": "PSA.15.4", "verseOrgIds": ["PSA.15.4"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 15:4"}, "items": [{"text": "he who keeps an oath even when it hurts, and doesn\u2019t change;", "type": "text", "attrs": {"verseId": "PSA.15.4", "verseOrgIds": ["PSA.15.4"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "he who doesn\u2019t lend out his money for usury,", "type": "text", "attrs": {"verseId": "PSA.15.5", "verseOrgIds": ["PSA.15.5"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 15:5"}, "items": [{"text": "nor take a bribe against the innocent.", "type": "text", "attrs": {"verseId": "PSA.15.5", "verseOrgIds": ["PSA.15.5"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "b", "vid": "PSA 15:5"}, "items": []}, {"name": "para", "type": "tag", "attrs": {"style": "q1", "vid": "PSA 15:5"}, "items": [{"text": "He who does these things shall never be shaken.", "type": "text", "attrs": {"verseId": "PSA.15.5", "verseOrgIds": ["PSA.15.5"]}}]}], "next": {"id": "PSA.16", "number": "16", "bookId": "PSA"}, "previous": {"id": "PSA.14", "number": "14", "bookId": "PSA"}}}}