{"_default": {"1": {"id": "PSA.1", "bibleId": "f72b840c855f362c-04", "number": "1", "bookId": "PSA", "reference": "Psalms 1", "copyright": "\n          \n            PUBLIC DOMAIN\n          \n        ", "content": [{"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Blessed is the man who doesn\u2019t walk in the counsel of the wicked,", "type": "text", "attrs": {"verseId": "PSA.1.1", "verseOrgIds": ["PSA.1.1"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 1:1"}, "items": [{"text": "nor stand on the path of sinners,", "type": "text", "attrs": {"verseId": "PSA.1.1", "verseOrgIds": ["PSA.1.1"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 1:1"}, "items": [{"text": "nor sit in the seat of scoffers;", "type": "text", "attrs": {"verseId": "PSA.1.1", "verseOrgIds": ["PSA.1.1"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "but his delight is in the LORD\u2019s", "type": "text", "attrs": {"verseId": "PSA.1.2", "verseOrgIds": ["PSA.1.2"]}}, {"text": " law.", "type": "text", "attrs": {"verseId": "PSA.1.2", "verseOrgIds": ["PSA.1.2"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 1:2"}, "items": [{"text": "On his Torah he meditates day and night.", "type": "text", "attrs": {"verseId": "PSA.1.2", "verseOrgIds": ["PSA.1.2"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "He will be like a tree planted by the streams of water,", "type": "text", "attrs": {"verseId": "PSA.1.3", "verseOrgIds": ["PSA.1.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 1:3"}, "items": [{"text": "that produces its fruit in its season,", "type": "text", "attrs": {"verseId": "PSA.1.3", "verseOrgIds": ["PSA.1.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 1:3"}, "items": [{"text": "whose leaf also does not wither.", "type": "text", "attrs": {"verseId": "PSA.1.3", "verseOrgIds": ["PSA.1.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 1:3"}, "items": [{"text": "Whatever he does shall prosper.", "type": "text", "attrs": {"verseId": "PSA.1.3", "verseOrgIds": ["PSA.1.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "The wicked are not so,", "type": "text", "attrs": {"verseId": "PSA.1.4", "verseOrgIds": ["PSA.1.4"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 1:4"}, "items": [{"text": "but are like the chaff which the wind drives away.", "type": "text", "attrs": {"verseId": "PSA.1.4", "verseOrgIds": ["PSA.1.4"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Therefore the wicked shall not stand in the judgment,", "type": "text", "attrs": {"verseId": "PSA.1.5", "verseOrgIds": ["PSA.1.5"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 1:5"}, "items": [{"text": "nor sinners in the congregation of the righteous.", "type": "text", "attrs": {"verseId": "PSA.1.5", "verseOrgIds": ["PSA.1.5"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "For the LORD knows the way of the righteous,", "type": "text", "attrs": {"verseId": "PSA.1.6", "verseOrgIds": ["PSA.1.6"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 1:6"}, "items": [{"text": "but the way of the wicked shall perish.", "type": "text", "attrs": {"verseId": "PSA.1.6", "verseOrgIds": ["PSA.1.6"]}}]}], "next": {"id": "PSA.2", "number": "2", "bookId": "PSA"}, "previous": {"id": "PSA.intro", "number": "intro", "bookId": "PSA"}}}}