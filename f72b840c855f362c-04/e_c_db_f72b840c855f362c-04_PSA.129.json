{"_default": {"1": {"id": "PSA.129", "bibleId": "f72b840c855f362c-04", "number": "129", "bookId": "PSA", "reference": "Psalms 129", "copyright": "\n          \n            PUBLIC DOMAIN\n          \n        ", "content": [{"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Many times they have afflicted me from my youth up.", "type": "text", "attrs": {"verseId": "PSA.129.1", "verseOrgIds": ["PSA.129.1"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 129:1"}, "items": [{"text": "Let Israel now say:", "type": "text", "attrs": {"verseId": "PSA.129.1", "verseOrgIds": ["PSA.129.1"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "many times they have afflicted me from my youth up,", "type": "text", "attrs": {"verseId": "PSA.129.2", "verseOrgIds": ["PSA.129.2"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 129:2"}, "items": [{"text": "yet they have not prevailed against me.", "type": "text", "attrs": {"verseId": "PSA.129.2", "verseOrgIds": ["PSA.129.2"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "The plowers plowed on my back.", "type": "text", "attrs": {"verseId": "PSA.129.3", "verseOrgIds": ["PSA.129.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 129:3"}, "items": [{"text": "They made their furrows long.", "type": "text", "attrs": {"verseId": "PSA.129.3", "verseOrgIds": ["PSA.129.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "The LORD is righteous.", "type": "text", "attrs": {"verseId": "PSA.129.4", "verseOrgIds": ["PSA.129.4"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 129:4"}, "items": [{"text": "He has cut apart the cords of the wicked.", "type": "text", "attrs": {"verseId": "PSA.129.4", "verseOrgIds": ["PSA.129.4"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Let them be disappointed and turned backward,", "type": "text", "attrs": {"verseId": "PSA.129.5", "verseOrgIds": ["PSA.129.5"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 129:5"}, "items": [{"text": "all those who hate Zion.", "type": "text", "attrs": {"verseId": "PSA.129.5", "verseOrgIds": ["PSA.129.5"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Let them be as the grass on the housetops,", "type": "text", "attrs": {"verseId": "PSA.129.6", "verseOrgIds": ["PSA.129.6"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 129:6"}, "items": [{"text": "which withers before it grows up,", "type": "text", "attrs": {"verseId": "PSA.129.6", "verseOrgIds": ["PSA.129.6"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "with which the reaper doesn\u2019t fill his hand,", "type": "text", "attrs": {"verseId": "PSA.129.7", "verseOrgIds": ["PSA.129.7"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 129:7"}, "items": [{"text": "nor he who binds sheaves, his bosom.", "type": "text", "attrs": {"verseId": "PSA.129.7", "verseOrgIds": ["PSA.129.7"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Neither do those who go by say,", "type": "text", "attrs": {"verseId": "PSA.129.8", "verseOrgIds": ["PSA.129.8"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 129:8"}, "items": [{"text": "\u201cThe blessing of the LORD be on you.", "type": "text", "attrs": {"verseId": "PSA.129.8", "verseOrgIds": ["PSA.129.8"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 129:8"}, "items": [{"text": "We bless you in the LORD\u2019s name.\u201d", "type": "text", "attrs": {"verseId": "PSA.129.8", "verseOrgIds": ["PSA.129.8"]}}]}], "next": {"id": "PSA.130", "number": "130", "bookId": "PSA"}, "previous": {"id": "PSA.128", "number": "128", "bookId": "PSA"}}}}