{"_default": {"1": {"id": "PSA.98", "bibleId": "f72b840c855f362c-04", "number": "98", "bookId": "PSA", "reference": "Psalms 98", "copyright": "\n          \n            PUBLIC DOMAIN\n          \n        ", "content": [{"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Sing to the LORD a new song,", "type": "text", "attrs": {"verseId": "PSA.98.1", "verseOrgIds": ["PSA.98.1"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 98:1"}, "items": [{"text": "for he has done marvelous things!", "type": "text", "attrs": {"verseId": "PSA.98.1", "verseOrgIds": ["PSA.98.1"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 98:1"}, "items": [{"text": "His right hand and his holy arm have worked salvation for him.", "type": "text", "attrs": {"verseId": "PSA.98.1", "verseOrgIds": ["PSA.98.1"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "The LORD has made known his salvation.", "type": "text", "attrs": {"verseId": "PSA.98.2", "verseOrgIds": ["PSA.98.2"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 98:2"}, "items": [{"text": "He has openly shown his righteousness in the sight of the nations.", "type": "text", "attrs": {"verseId": "PSA.98.2", "verseOrgIds": ["PSA.98.2"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "He has remembered his loving kindness and his faithfulness toward the house of Israel.", "type": "text", "attrs": {"verseId": "PSA.98.3", "verseOrgIds": ["PSA.98.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 98:3"}, "items": [{"text": "All the ends of the earth have seen the salvation of our God.", "type": "text", "attrs": {"verseId": "PSA.98.3", "verseOrgIds": ["PSA.98.3"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Make a joyful noise to the LORD, all the earth!", "type": "text", "attrs": {"verseId": "PSA.98.4", "verseOrgIds": ["PSA.98.4"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 98:4"}, "items": [{"text": "Burst out and sing for joy, yes, sing praises!", "type": "text", "attrs": {"verseId": "PSA.98.4", "verseOrgIds": ["PSA.98.4"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Sing praises to the LORD with the harp,", "type": "text", "attrs": {"verseId": "PSA.98.5", "verseOrgIds": ["PSA.98.5"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 98:5"}, "items": [{"text": "with the harp and the voice of melody.", "type": "text", "attrs": {"verseId": "PSA.98.5", "verseOrgIds": ["PSA.98.5"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "With shofars", "type": "text", "attrs": {"verseId": "PSA.98.6", "verseOrgIds": ["PSA.98.6"]}}, {"text": " and sound of the ram\u2019s horn,", "type": "text", "attrs": {"verseId": "PSA.98.6", "verseOrgIds": ["PSA.98.6"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 98:6"}, "items": [{"text": "make a joyful noise before the King, the LORD.", "type": "text", "attrs": {"verseId": "PSA.98.6", "verseOrgIds": ["PSA.98.6"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Let the sea roar with its fullness;", "type": "text", "attrs": {"verseId": "PSA.98.7", "verseOrgIds": ["PSA.98.7"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 98:7"}, "items": [{"text": "the world, and those who dwell therein.", "type": "text", "attrs": {"verseId": "PSA.98.7", "verseOrgIds": ["PSA.98.7"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Let the rivers clap their hands.", "type": "text", "attrs": {"verseId": "PSA.98.8", "verseOrgIds": ["PSA.98.8"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 98:8"}, "items": [{"text": "Let the mountains sing for joy together.", "type": "text", "attrs": {"verseId": "PSA.98.8", "verseOrgIds": ["PSA.98.8"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1"}, "items": [{"text": "Let them sing before the LORD,", "type": "text", "attrs": {"verseId": "PSA.98.9", "verseOrgIds": ["PSA.98.9"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 98:9"}, "items": [{"text": "for he comes to judge the earth.", "type": "text", "attrs": {"verseId": "PSA.98.9", "verseOrgIds": ["PSA.98.9"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q1", "vid": "PSA 98:9"}, "items": [{"text": "He will judge the world with righteousness,", "type": "text", "attrs": {"verseId": "PSA.98.9", "verseOrgIds": ["PSA.98.9"]}}]}, {"name": "para", "type": "tag", "attrs": {"style": "q2", "vid": "PSA 98:9"}, "items": [{"text": "and the peoples with equity.", "type": "text", "attrs": {"verseId": "PSA.98.9", "verseOrgIds": ["PSA.98.9"]}}]}], "next": {"id": "PSA.99", "number": "99", "bookId": "PSA"}, "previous": {"id": "PSA.97", "number": "97", "bookId": "PSA"}}}}